{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "WalletAppPostgreSqlServer": "<stored in user-secrets>"
  },
  "Auth": {
    "Token": {
      "Secret": "<stored-in-user-secrets>",
      "Issuer": "<stored-in-user-secrets>",
      "Audience": "<stored-in-user-secrets>",
      "ExpirationTimeSeconds": 43200
    }
  },
  "Identity": {
    "RequireUniqueEmail": true,
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 8,
      "RequireLowercase": true,
      "RequireNonAlphanumeric": true,
      "RequireUppercase": true,
      "RequiredUniqueChars": 5
    }
  }
}